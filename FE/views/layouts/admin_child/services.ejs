<script>
    $(document).ready(function(){
        $.ajax({
            url:'/admin/group/list',
            type:'get',
            headers: {
                authorization: "Bearer " + getACCESS_TOKEN(),
            },
            success:function(data){
                $('#sbServices').empty();
                $('#sbServices').append('<h6 class="collapse-header">Services list</h6>');

                data.groups.forEach(g=>{
                    $('#sbServices').append(`<a class="collapse-item" href="/admin/services/${g.metatitle}">${g.name}</a>`);
                })           
            }
        })
    })
</script>